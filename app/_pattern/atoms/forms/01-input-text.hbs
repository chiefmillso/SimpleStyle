<fieldset>
    <div>
        <label for="{{id}}-text">input text</label><input id="{{id}}-text" type="text" placeholder="text input">
    </div>
    <div>
        <label for="{{id}}-password">input password</label><input id="{{id}}-password" type="password">
    </div>
    <div>
        <label for="{{id}}-checkbox">input checkbox</label><input id="{{id}}-checkbox" type="checkbox">
    </div>
    <div>
        <label for="{{id}}-checkbox-checked">input checkbox checked</label><input id="{{id}}-checkbox-checked" type="checkbox" checked>
    </div>
    <div>
        <label for="{{id}}-radio">input radio</label><input id="{{id}}-radio" type="radio">
    </div>
    <div>
        <label for="{{id}}-url">input url</label><input id="{{id}}-url" type="url" placeholder="url input">
    </div>
    <div>
        <label for="{{id}}-email">input url</label><input id="{{id}}-email" type="email" placeholder="email input">
    </div>
    <div>
        <label for="{{id}}-search">input url</label><input id="{{id}}-search" type="search" placeholder="search input">
    </div>
</fieldset>